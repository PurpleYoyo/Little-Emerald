script EventScript_GameCorner_Clerk_TMs {
	lockall
	msgbox(format("I sell TMs in exchange for 2 Battle Points."), MSGBOX_AUTOCLOSE)
	specialvar(VAR_TEMP_1, GetFrontierBattlePoints)
	if (var(VAR_TEMP_1) < 2) { // Not Enough BP
		msgbox(format("You don't have enough Battle Points. Come back when you have some more."), MSGBOX_AUTOCLOSE)
		closemessage
		releaseall
		end
	}
	msgbox(format("Would you like to buy anything?"), MSGBOX_YESNO)
