script CheaperBPMoveTutor {
	lockall
	faceplayer
	msgbox(format("I can teach your Pokémon a good move in exchange for a Battle Point each."), MSGBOX_AUTOCLOSE)
	specialvar(VAR_TEMP_1, GetFrontierBattlePoints)
	if (var(VAR_TEMP_1) < 1) { // Not Enough BP
		msgbox(format("You don't have enough Battle Points. Come back later when you have some more."), MSGBOX_AUTOCLOSE)
		closemessage
		releaseall
		end
	}
	msgbox(format("Would you like me to teach your Pokémon a move?"), MSGBOX_YESNO)
	if (var(VAR_RESULT) == 0) { // No
		msgbox(format("I'll be here if you change your mind."), MSGBOX_AUTOCLOSE)
		closemessage
		releaseall
		end
	}
	closemessage
	special(ShowBattlePointsWindow)
	setvar(VAR_0x8004, SCROLL_MULTI_BATTLE_TENT_RULES)
	special(ShowScrollableMultichoice)
	waitstate
	special(CloseBattlePointsWindow)
	switch (var(VAR_RESULT)) {
		default:
			msgbox(format("I'll be here if you change your mind."), MSGBOX_AUTOCLOSE)
			closemessage
			end
		case 0:
			setvar(VAR_0x8005, MOVE_CRUNCH)
